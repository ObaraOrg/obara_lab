###############################################################################

EXE     = sss2
RM      = /bin/rm -f
ZIP	= zip

DATE 	= -DMC_COMPILE_DATE="\"`date `\""

# Version number added:

BKFILE 	= "`date +'backup/serpent_2.1.0_%y%m%d%H%M.zip'`"
EXFILE  = "`date +'backup/serpent_2.1.0_%y%m%d%H%M'`"

###############################################################################

$(EXE): $(OBJS) 
	$(CC) $(OBJS) $(LDFLAGS) -o $(EXE)

bk:
	$(ZIP) $(BKFILE) *.c *.h Makefile versions.txt $(EXE)
	cp $(EXE) $(EXFILE)
	cp $(BKFILE) ./serpent2.zip
	chmod a-w $(BKFILE)
	chmod a-w $(EXFILE)

clean:	
	$(RM) $(OBJS) $(OBSOLETE)

###############################################################################

